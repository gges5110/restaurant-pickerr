<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

  <% include ../partials/nav.ejs %>

    <div class="container">

      <h1>Get Address from Geocode.</h1>

      <form id="geocoding_form" class="form-horizontal" action="/geocode">
        <div class="form-group">
          <label for="lat_input" class="col-sm-2 col-sm-offset-2 control-label">Latitude</label>
          <div class="col-sm-6">
              <input type="number" id="lat_input" name="lat" class="form-control">
          </div>
        </div>
        <div class="form-group">
          <label for="lng_input" class="col-sm-2 col-sm-offset-2 control-label">Longitude</label>
          <div class="col-sm-6">
              <input type="number" id="lng_input" name="long" class="form-control">
          </div>
        </div>

        <div>
        <input type="submit" value="Submit" class="btn btn-info">
      </form>

    </div>



</body>
</html>
